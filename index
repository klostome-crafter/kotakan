    <script>
        // Smooth scrolling for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                
                const targetId = this.getAttribute('href');
                if(targetId === '#') return;
                
                const targetElement = document.querySelector(targetId);
                if(targetElement) {
                    window.scrollTo({
                        top: targetElement.offsetTop - 80,
                        behavior: 'smooth'
                    });
                }
            });
        });
        
        // WhatsApp redirect with pre-filled message
        document.querySelectorAll('a[href*="wa.me"]').forEach(link => {
            link.addEventListener('click', function(e) {
                // You can customize the pre-filled message here
                const message = "Halo, saya tertarik dengan layanan pembuatan box custom. Bisa konsultasi lebih lanjut?";
                this.href = `https://wa.me/628974420966?text=${encodeURIComponent(message)}`;
            });
        });
    </script>
